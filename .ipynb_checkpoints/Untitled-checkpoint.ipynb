   "source": [
    "hour = [1,2,3]\n",
    "point = [2,4,6]\n",
    "op = [0,0,0]\n",
    "\n",
    "def gety(data, label,x,op):\n",
    "    w = 0\n",
    "    b = 0\n",
    "    for i in range(0,3):\n",
    "        w+=(label[i]-op[i])/data[i]\n",
    "        b +=op[i]\n",
    "    w = w/3\n",
    "    b = b/3\n",
    "    return x*w + b\n",
    "\n",
    "gety(hour,point,4,b)"
